# Configuración de Supabase para InternetPerla SGI
# Obtén estos valores desde tu proyecto Supabase en https://app.supabase.com

# URL del proyecto Supabase (Project URL)
SUPABASE_URL=https://tu-proyecto-id.supabase.co

# Clave anónima/pública de Supabase (anon key) - para el frontend
SUPABASE_ANON_KEY=eyJhbGci...

# Clave de servicio de Supabase (service_role key) - para el backend
SUPABASE_SERVICE_KEY=eyJhbGci...

# Configuración de la base de datos PostgreSQL
SUPABASE_DB_HOST=db.tu-proyecto-id.supabase.co
SUPABASE_DB_PORT=5432
SUPABASE_DB_NAME=postgres
SUPABASE_DB_USERNAME=postgres
SUPABASE_DB_PASSWORD=tu-password-de-db

# Configuración SSL (requerida para Supabase)
SUPABASE_DB_SSL=true
